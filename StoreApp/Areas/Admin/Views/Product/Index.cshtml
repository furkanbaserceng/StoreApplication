


@using Entities.Models

<h1> Product Index</h1>
@model IEnumerable<Product>

    <table class="table">

    @foreach(var item in Model)
    {
        <tr>
            <td> @item.ProductId </td>
            <td> @item.ProductName </td>
            <td> @item.Price </td>


        </tr>

    }
    

    </table>

