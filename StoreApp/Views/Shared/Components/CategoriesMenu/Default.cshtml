
@model IEnumerable<Category>

<div class="my-4">

    <ul class="list-group">

        @foreach (var item in Model)
        {
            <li class="list-group-item"> <a class="link" asp-controller="Product" asp-action="GetByCategory" asp-route-id="@item.CategoryId">@item.CategoryName</a>  </li>
            
        }

    </ul>


</div>