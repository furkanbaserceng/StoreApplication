
@model IEnumerable<Category>


    <div class="text-center my-3">

        <h6 class="display-6 my-2">Categories</h6>

    </div>

<div class="container-fluid">
    <ul class="list-group">
        @foreach (var item in Model)
        {
            <li class="list-group-item"> @item.CategoryName </li>

        }

    </ul>
</div>
